@model DataAccess1.Model.ContactModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="cs">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="Josef" />
    <title>@ViewData["Title"] - Rent a car</title>
    <!-- Favicon-->
    <link rel="icon" type="image/x-icon" href="@Url.Content("~/Scripts/img/favicon.ico")" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v5.15.1/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="@Url.Content("~/Scripts/css/styles1.css")" rel="stylesheet" />



</head>
<body id="page-top">
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark text-uppercase fixed-top" id="mainNav">
        <div class="container">
            <p>607123654</p>
            <a class="navbar-brand js-scroll-trigger" href="@Url.Action("Index","Home", new {area= "" })">Autopůjčovna</a>
            <button class="navbar-toggler navbar-toggler-right text-uppercase font-weight-bold bg-primary text-white rounded" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                Menu
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="@Url.Action("Index","Items",new {area="Admin"})">Přihlášení</a></li>
                    <li class="nav-item mx-0 mx-lg-1"><a class="nav-link py-3 px-0 px-lg-3 rounded js-scroll-trigger" href="@Url.Action("Index","Items")">Přehled aut</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <!-- Masthead-->
    <header class="masthead-heading" id="avatar">

        <div class="d-flex align-items-center flex-column px-0">

            <!-- Masthead Avatar Image masthead-avatar mb-12-->
            <img class="img-fluid" src="@Url.Content("~/Scripts/img/portfolio/843.png")" alt="fotka auta" />
        </div>
    </header>
    @RenderBody()
   
    <!-- Footer-->
    <footer class="footer text-center navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container">
            <div class="row">
                <!-- Footer Location-->
                <div class="col-lg-4 mb-5 mb-lg-0">
                    <h4 class="text-uppercase mb-4">Lokace</h4>
                    <p class="lead mb-0">
                        UHK FIM Hradec Králové
                        <br />
                        CZE
                    </p>
                </div>
                <!-- Footer Social Icons-->
                <div class="col-lg-4 mb-5 mb-lg-0">
                    <h4 class="text-uppercase mb-4">Okolní weby</h4>
                    <a class="btn btn-outline-light btn-social mx-1" href="www.facebook.com"><i class="fab fa-fw fa-facebook-f"></i></a>
                    <a class="btn btn-outline-light btn-social mx-1" href="#!"><i class="fab fa-fw fa-twitter"></i></a>
                    <a class="btn btn-outline-light btn-social mx-1" href="#!"><i class="fab fa-fw fa-linkedin-in"></i></a>
                    <a class="btn btn-outline-light btn-social mx-1" href="#!"><i class="fab fa-fw fa-dribbble"></i></a>
                </div>
                <!-- Footer About Text-->
                <div class="col-lg-4">
                    <h4 class="text-uppercase mb-4">O nás</h4>
                    <p class="lead mb-0">
                        Tato šablona byla použita na základě licence, MIT licensed Bootstrap theme created by
                        <a href="http://startbootstrap.com"> </a>
                        &copy; @DateTime.Now.Year; Josef Janda for UHK

                    </p>
                </div>
            </div>
        
        </div>
    </footer>

    <!-- Scroll to Top Button (Only visible on small and extra-small screen sizes)-->
    <div class="scroll-to-top d-lg-none position-fixed">
        <a class="js-scroll-trigger d-block text-center text-white rounded" href="#page-top"><i class="fa fa-chevron-up"></i></a>
    </div>


    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Contact form JS-->
    <script src="@Url.Content("~/Scripts/mail/jqBootstrapValidation.js")"></script>
    <script src="@Url.Content("~/Scripts/mail/contact_me.js")"></script>
    <!-- Core theme JS-->
    <script src="@Url.Content("~/Scripts/js/scripts.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

</body>
</html>

